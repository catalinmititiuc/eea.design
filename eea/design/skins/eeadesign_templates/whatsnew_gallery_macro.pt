<tal:news metal:define-macro="propertyproducts"
        tal:define="frontpage_view frontpage_view|context/@@frontpage_highlights;
                    product python:frontpage_view.getPropertyProduct(name=request.get('tabname'));
                    folderContents folderContents|python:frontpage_view.getPropertyProductContent(product) if product else None;
                    hide_listing hide_listing|python: False;
                    hide_album hide_album|python: True;
                    ajax ajax|python:True">

    <tal:folderContents condition="folderContents">
        <div metal:use-macro="here/frontpage_macros/macros/gallery-condition" />
    </tal:folderContents>

    <metal:languages tal:condition="not:folderContents">
        <metal:results use-macro="here/articles_gallery_macro/macros/noresults" />
    </metal:languages>

</tal:news>

