<?xml version="1.0"?>
<object name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="eea">
    <configlet title="Mini Header settings"
        action_id="EEAMiniHeader"
        appId="eea.design"
        category="Products"
        conditional_expression=""
        icon_expr="string:${portal_url}/logoIcon.png"
        url_expr="string:${portal_url}/@@eea-miniheader-controlpanel"
        visible="True"
        i18n:attributes="title">
        <permission>Manage portal</permission>
    </configlet>
</object>
